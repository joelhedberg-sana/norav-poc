{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Run EcgUi",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net8.0/EcgUi.dll",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": { "ASPNETCORE_URLS": "http://localhost:5200" }
    }
  ]
}
